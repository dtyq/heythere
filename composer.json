{
    "name": "dtyq/heythere",
    "description": "一个简单的问候工具包，帮助您以多种语言向用户打招呼",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Twosee",
            "email": "twosee@qq.com"
        }
    ],
    "require": {
        "php": ">=8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Twosee\\HeyThere\\": "src/"
        }
    },
    "autoload-dev": {
